{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/server.js",
    "dev": "nodemon --watch src --ext ts --exec \"ts-node -r tsconfig-paths/register src/server.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "seed:neo4j": "ts-node src/db/seeders/seedCourses.ts",
    "seed:pg": "ts-node -r tsconfig-paths/register src/db/seeders/index.ts",
    "seed": "pnpm seed:pg && pnpm seed:neo4j"
  },
  "license": "ISC",
  "dependencies": {
    "@pnpm/logger": "^1001.0.1",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "neo4j-driver": "^6.0.1",
    "nodemailer": "^7.0.11",
    "pg": "^8.16.3",
    "socket.io": "^4.8.1",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^6.0.2",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.15.6",
    "@types/winston": "^2.4.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "prisma": "^7.0.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
